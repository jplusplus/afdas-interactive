html
  //<![endif]
  head
    meta(http-equiv="X-UA-Compatible",content="IE=edge,chrome=1")
    meta(name="viewport",content="width=1200")
    meta(charset="utf-8")
    if typeof title !== "undefined"
        title=title
    else
        title AFDAS

    block stylesheets
    | <style type='text/css'>
    :less(minify=true)
        @import url("public/css/style.less");
    | </style>

  body.js-loading.js-first(class=data.class)
    #old-browser
        h4 Votre navigateur <strong>est trop ancien</strong>
        p <a href="http://browsehappy.com/">Mettez à jour votre navigateur</a> ou <a href="http://www.google.com/chromeframe/?redirect=true">installez Google Chrome Frame</a> pour accéder à cette page.


    #overflow(data-navigation=data.navigation || "horizontal")
        include widget/nav.jade
        #analyse.hidden
            .overlay.js-analyse-toggler
            .dialog
                h3 L'analyse de l'observatoire
                a.js-analyse-toggler.close
                    span.hide-text Fermer
                #analyse-content.wrapper
        #container
            block content

    img.hidden(src="./img/carte-2013/fb.jpg")


    != js('global')
    != js('addon')
    if debugMode
        script(src="/js/vendor/jquery-ui.min.js", type="text/javascript")
        != js('debug')

    block scripts
